    <script>
        const app = new Vue ({
            el: '#app',
            data: {
              montant: 1000 ,
              NbreMois: 12,
              taux: 0.0189
            },
            computed:{
                Total : function(){
                  this.montant = parseInt(this.montant)
                  this.NbreMois = parseInt(this.NbreMois)
                  this.taux = parseFloat(this.taux)

                  return parseInt((this.montant * this.NbreMois * this.taux) + this.montant) 

                },
                parMois : function (){
                  return parseInt(this.Total/this.NbreMois)  + 1 
                }
            }
                    
        })
    </script>

